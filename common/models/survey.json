{
  "name": "survey",
  "plural": "surveys",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "mongodb": {
      "collection": "surveys"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "expires": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "expiresAt": {
      "type": "date",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user"
    },
    "questions": {
      "type": "hasMany",
      "model": "question",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": []
}
