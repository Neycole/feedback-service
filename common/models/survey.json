{
  "name": "survey",
  "plural": "surveys",
  "base": "PersistedModel",
  "idInjection": true,
  "strict": true,
  "options": {
    "validateUpsert": true,
    "mongodb": {
      "collection": "surveys"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": false,
      "defaultFn": "now"
    },
    "expires": {
      "type": "boolean",
      "required": true
    },
    "expiresAt": {
      "type": "date",
      "required": false
    },
    "questions": [
      {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "generated": true,
            "id": true
          },
          "text": {
            "type": "string",
            "required": true
          },
          "order": {
            "type": "integer",
            "required": true
          },
          "type": {
            "type": "string",
            "required": true
          },
          "multiple": {
            "type": "boolean",
            "required": false,
            "default": false
          },
          "answers": [
            {
              "_id": {
                "type": "string",
                "generated": true,
                "id": true
              },
              "text": {
                "type": "string",
                "required": true
              },
              "order": {
                "type": "integer",
                "required": true
              }
            }
          ]
        }
      }
    ]
  },
  "validations": [],
  "relations": {
    "user": {
      "model": "User",
      "type": "belongsTo"
    }
  },
  "acls": [],
  "methods": []
}
